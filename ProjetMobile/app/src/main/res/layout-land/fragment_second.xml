<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.google.android.gms.maps.MapView
        android:id="@+id/map"
        android:layout_width="316dp"
        android:layout_height="196dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintDimensionRatio="h,1:1"
        card_view:layout_constraintHorizontal_bias="0.1"
        card_view:layout_constraintLeft_toLeftOf="parent"
        card_view:layout_constraintRight_toLeftOf="@+id/imageView4"
        card_view:layout_constraintTop_toTopOf="parent"
        card_view:layout_constraintVertical_bias="0.494"
        tools:layout_editor_absoluteY="46dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="83dp"
        android:layout_height="86dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintHorizontal_bias="0.79"
        card_view:layout_constraintLeft_toLeftOf="parent"
        card_view:layout_constraintRight_toRightOf="parent"
        card_view:srcCompat="@drawable/contact"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        card_view:layout_constraintTop_toTopOf="@+id/map"
        android:layout_marginTop="0dp"
        card_view:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/apropo"
        android:textAlignment="center"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintHorizontal_bias="0.5"
        card_view:layout_constraintRight_toRightOf="@+id/textView5"
        card_view:layout_constraintTop_toBottomOf="@+id/imageView4"
        card_view:layout_constraintVertical_bias="0.0"
        card_view:layout_constraintLeft_toLeftOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="75dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginStart="75dp"
        android:background="@drawable/rounded1"
        android:gravity="center"
        android:padding="14dp"
        android:text="@string/title_call"
        android:textColor="@color/colorPrimary"
        android:textSize="18sp"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintHorizontal_bias="0.5"
        card_view:layout_constraintLeft_toLeftOf="@+id/imageView4"
        card_view:layout_constraintRight_toRightOf="@+id/imageView4"
        card_view:layout_constraintTop_toBottomOf="@+id/textView"
        card_view:layout_constraintVertical_bias="0.441" />


</android.support.constraint.ConstraintLayout>
